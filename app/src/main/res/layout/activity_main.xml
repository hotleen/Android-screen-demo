    <ProgressBar
        android:id="@+id/progressbar"
        android:layout_width="@dimen/dp_100"
        android:layout_height="@dimen/dp_100"
        android:layout_centerInParent="true"
        android:visibility="gone" />

    <!--提示信息弹框-->
    <include layout="@layout/layout_fh_hint" />

</RelativeLayout>
