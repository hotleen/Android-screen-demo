<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_999</item>
        <item name="colorPrimaryDark">@color/color_999</item>
        <item name="colorAccent">@color/fh_title_bg</item>
    </style>

    <style name="FHlive" parent="AppTheme">
        <item name="android:windowBackground">@mipmap/ic_fh_teller_is_audio</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="tv_title">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="et_home">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">110dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/btn_blue_huan_5</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="btn_home">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_blue_bg_5</item>
    </style>

    <style name="rl_main_bot">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp_55</item>
        <item name="android:background">#ffffff</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="iv_main_bot">
        <item name="android:layout_marginTop">@dimen/dp_5</item>
        <item name="android:padding">@dimen/dp_5</item>
        <item name="android:layout_width">@dimen/dp_30</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_height">@dimen/dp_30</item>
    </style>

    <style name="tv_main_bot">
        <item name="android:layout_marginTop">@dimen/dp_30</item>
        <item name="android:layout_width">@dimen/dp_30</item>
        <item name="android:layout_height">@dimen/dp_20</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@color/color_999</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="tv_login_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_44</item>
        <item name="android:layout_marginLeft">@dimen/dp_25</item>
        <item name="android:layout_marginRight">@dimen/dp_25</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textSize">@dimen/sp_16</item>
    </style>



    <style name="tv_login_bot_route">
        <item name="android:layout_width">@dimen/dp_48</item>
        <item name="android:layout_height">@dimen/dp_22</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_88</item>
        <item name="android:textSize">@dimen/sp_10</item>
        <item name="android:textColor">#c3c8d5</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="tv_setting_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_45</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingTop">@dimen/dp_12</item>
        <item name="android:paddingLeft">@dimen/dp_28</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">@color/color_666</item>
    </style>

    <style name="tv_setting_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_56</item>
        <item name="android:textColor">@color/color_333</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/dp_28</item>
    </style>

    <style name="iv_setting_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginLeft">@dimen/dp_28</item>
        <item name="android:layout_marginRight">@dimen/dp_28</item>
    </style>

    <style name="iv_setting_check">
        <item name="android:layout_marginRight">@dimen/dp_30</item>
        <item name="android:layout_width">@dimen/dp_20</item>
        <item name="android:layout_height">@dimen/dp_20</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@mipmap/ic_fh_setting_check</item>
    </style>

    <style name="et_setting_url">
        <item name="android:layout_marginLeft">@dimen/dp_120</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_56</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:layout_marginRight">@dimen/dp_28</item>
        <item name="android:textColorHint">@color/color_999</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textColor">@color/color_333</item>
        <item name="android:background">@color/fh_color_trans</item>
    </style>

</resources>
